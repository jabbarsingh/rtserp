        <TextBox   KeyDown="Window_KeyDown" x:Name="ItemDesc" HorizontalAlignment="Left" Height="23" Margin="90,144,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="145"/>

