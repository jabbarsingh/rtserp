        <ComboBox   KeyDown="Window_CustKeyDown" SelectedItem="Watch"  GotFocus="GroupName_GotFocus" SelectionChanged="GroupName_SelectionChanged" x:Name="GroupName"  HorizontalAlignment="Left" Height="27" Margin="123,226,0,0"  VerticalAlignment="Top" Width="110"/>
        <ComboBox  KeyDown="Window_CustKeyDown"  x:Name="SubGroupName"  HorizontalAlignment="Left" Height="28" Margin="124,258,0,0"  VerticalAlignment="Top" Width="109"/>
        <TextBox KeyDown="Window_CustKeyDown"  GotFocus="TextBox_GotFocus" x:Name="ItemPrice"  PreviewTextInput="NumberValidationTextBox"  HorizontalAlignment="Left" Height="23" Margin="123,291,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="110"/>
        <TextBox  KeyDown="Window_CustKeyDown"  GotFocus="TextBoxHighlight_GotFocus"  LostFocus="TextBoxHighlight_LostFocus" x:Name="HSN"  PreviewTextInput="NumberValidationTextBox"  HorizontalAlignment="Left" Height="23" Margin="123,319,0,0" TextWrapping="Wrap" Text="7113" VerticalAlignment="Top" Width="109"/>
        <TextBox Foreground="White" Background="Black"  GotFocus="TextBoxHighlight_GotFocus"  LostFocus="TextBoxHighlight_LostFocus"  KeyDown="Window_CustKeyDown"  x:Name="GSTRate"  PreviewTextInput="NumberValidationTextBox"  HorizontalAlignment="Left" Height="23" Margin="124,347,0,0" TextWrapping="Wrap" Text="18" VerticalAlignment="Top" Width="109"/>
        <Button LostFocus="AddItemRow_LostFocus" GotFocus="AddItemRow_GotFocus"   Content="_Submit" HorizontalAlignment="Left" Margin="124,380,0,0" VerticalAlignment="Top" Width="117" Height="45" Click="Button_Click_2"/>
        <Label Content="Price" HorizontalAlignment="Left" Margin="64,293,0,0" VerticalAlignment="Top" Width="42" Height="26"  FontWeight="Bold" />
        <Label  FontWeight="Bold" Content="HSN" HorizontalAlignment="Left" Margin="71,319,0,0" VerticalAlignment="Top" Width="38" Height="26"/>
        <Label  FontWeight="Bold"  Content="GST Rate" HorizontalAlignment="Left" Margin="50,347,0,0" VerticalAlignment="Top" Width="65" Height="26"/>

        <Label Content="Qty" Margin="64,97,261,0" VerticalAlignment="Top" Height="26" FontWeight="Bold" />
        <Label FontWeight="Bold"  Content="Group" HorizontalAlignment="Left" Margin="64,229,0,0" VerticalAlignment="Top" Width="51" Height="26"/>
        <Label  FontWeight="Bold"  Content="Sub Group" HorizontalAlignment="Left" Margin="50,258,0,0" VerticalAlignment="Top" Width="69" Height="26"/>

        <Label FontWeight="Bold" Content="Barcode" HorizontalAlignment="Left" Margin="61,39,0,0" VerticalAlignment="Top" Width="61" Height="27"/>

        <DatePicker x:Name="FinYeraStartDate" HorizontalAlignment="Left" Margin="256,10,0,0" SelectedDate="{x:Static System:DateTime.Now}" VerticalAlignment="Top" Width="104"/>
        <GroupBox Header="Item Details" HorizontalAlignment="Left" Margin="0,-5,-1,0" VerticalAlignment="Top" Height="430" Width="371"/>
 <Label  FontWeight="Bold"  Content="Weight" HorizontalAlignment="Left" Margin="64,132,0,0" VerticalAlignment="Top" Width="58" Height="26"/>
        <Label  FontWeight="Bold"  Content="Wastage %" HorizontalAlignment="Left" Margin="44,167,0,0" VerticalAlignment="Top" Width="74" Height="26"/>
        <Label  FontWeight="Bold"  Content="MC-Charge" HorizontalAlignment="Left" Margin="44,198,0,0" VerticalAlignment="Top" Width="74" Height="26"/>

    </Grid>
