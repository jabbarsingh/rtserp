<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Background="#f5bd88"         ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="RTSJewelERP.AddInstantBarcodeJewell"
        Title="AddInstantBarcodeItem" Height="454" Width="376">
    <Grid>
        <!--<ComboBox x:Name="taxtype"  HorizontalAlignment="Right" Height="23" Margin="0,78,360,0"  VerticalAlignment="Top" Width="87" Grid.Column="1">
            <ComboBoxItem Tag="L" IsSelected="True" Content="VAT(%)"/>
            <ComboBoxItem Tag="V" Content="CST(%)"/>
            <ComboBoxItem Tag="T" Content="GST(%)"/>
        </ComboBox>-->
        <Label FontWeight="Bold" Content="Name" HorizontalAlignment="Left" Margin="69,67,0,0" VerticalAlignment="Top" Width="51" Height="27"/>



        <TextBox FontSize="13" TabIndex="0" KeyDown="Window_CustKeyDown" TextChanged="Barcode_TextChanged" Grid.Column="0" Grid.Row="0"   x:Name="txtBarcode" Height="23" Margin="122,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Left" Width="153"/>
        <TextBox  TabIndex="1" GotFocus="TextBoxHighlight_GotFocus"  LostFocus="TextBoxHighlight_LostFocus"   KeyDown="Window_KeyDown" x:Name="autocompleteItemNameStockEntry" HorizontalAlignment="Left" Height="28" Margin="122,67,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="231"/>
        <TextBox  KeyDown="Window_CustKeyDown"  x:Name="ActualQty"  PreviewTextInput="NumberValidationTextBox"  HorizontalAlignment="Left" Height="23" Margin="123,100,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="34"/>
        <TextBox Foreground="White" Background="Black"   GotFocus="TextBoxHighlight_GotFocus"  LostFocus="TextBoxHighlight_LostFocus" KeyDown="Window_CustKeyDown"    x:Name="actualWt"  PreviewTextInput="NumberValidationTextBox"  HorizontalAlignment="Left" Height="23" Margin="122,136,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="99"/>
