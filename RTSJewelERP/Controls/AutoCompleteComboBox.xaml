<UserControl x:Class="RTSJewelERP.Controls.AutoCompleteComboBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
    Panel.ZIndex="10">
    <Canvas>
        <TextBox x:Name="autoTextBox" TabIndex="0"  FontSize="20"    Background="Black"  
                 Height="54" MinWidth="150" TextWrapping="NoWrap" Foreground="Wheat" FontWeight="Bold" Width="253" Text="Cash" />
        <ListBox x:Name="suggestionListBox" 
                 SelectionChanged="suggestionListBox_SelectionChanged"
                 Background="SeaGreen"   Foreground="White" FontSize="18"  
                 Visibility="Collapsed" 
                 MinWidth="150" 
                 Margin="0,25,0,-25"/>
    </Canvas>
</UserControl>
