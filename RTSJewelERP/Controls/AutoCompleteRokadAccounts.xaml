<UserControl x:Class="RTSJewelERP.Controls.AutoCompleteRokadAccounts"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
    Panel.ZIndex="10" Height="46" Width="225">
    <Canvas>
        <TextBox x:Name="autoTextBoxRokad" TabIndex="0"  FontSize="18"  FontWeight="Bold"   Background="#f5bd88"  
                 Height="46" MinWidth="150" TextWrapping="NoWrap" Width="227" Text="Cash" />
        <ListBox x:Name="suggestionListBox" 
                 SelectionChanged="suggestionListBox_SelectionChanged"
                 Background="SeaGreen"   Foreground="White" FontSize="18"  
                 Visibility="Collapsed" 
                 MinWidth="150" 
                 Margin="0,25,0,-25"/>
    </Canvas>
</UserControl>
