<UserControl x:Class="RTSJewelERP.Controls.AutoCompleteTextboxStockSubGroup"
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
    Panel.ZIndex="10">
    <Canvas>
        <TextBox x:Name="autoTextBoxStockSubGroup" TabIndex="0" Foreground="Black"  FontWeight="Bold" Background="#f5bd88"  
                 Height="25" TextWrapping="NoWrap" Width="107" Text="" />
        <ListBox x:Name="suggestionListBox" 
                 SelectionChanged="suggestionListBox_SelectionChanged"
                 Background="SeaGreen" 
                 Visibility="Collapsed" 
                 MinWidth="150" 
                 Margin="0,25,0,-25"/>
    </Canvas>
</UserControl>

