<UserControl x:Class="RTSJewelERP.Controls.AutoCompleteTextboxStockEntryItemxaml"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
    Panel.ZIndex="10">
    <Canvas>
        <TextBox x:Name="autoTextBoxStockEntry" TabIndex="0"  Foreground="White" FontWeight="Bold" FontSize="18" 
                 Height="25" MinWidth="150" TextWrapping="NoWrap" Width="218" Text="" />
        <ListBox x:Name="suggestionListBox" 
                 SelectionChanged="suggestionListBox_SelectionChanged"
                 Background="SeaGreen"  Foreground="White" FontSize="18"  
                 Visibility="Collapsed" 
                 MinWidth="150" 
                 Margin="0,25,0,-25"/>
    </Canvas>
</UserControl>
